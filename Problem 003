/*

//Largest prime factor
//Unsolved

#include <iostream>
#include <string>
#include <sstream>

using namespace std;


int prime(int n) {
    int x = 2;
    bool boo = false;
    if (n == 2) {
        boo = true;
        return boo;
    }
    while (n % x != 0) {
        ++x;
        if ((n - 1) == x) {
            boo = true;
        }
    } return boo;
}

int main () {
    int n;
    int p = 2;
    cout << "Enter number: ";
    cin >> n;
    while (n > p) {
        if (prime(p)) {
            if (n % p == 0) {
                n = n / p;
                cout << p << endl;;
            } ++p;
        } ++p;
    } cout << n;
    return 0;
}
