//Largest prime factor
//Unsolved

#include <iostream>

using namespace std;

int factors(int x) {
    for (int i = 2; i < x; i++) {
        if (x % i == 0) {
            x = x / i;
            return x;
        }
    }
    return x;
}

int main() {
    int number;
    cout << "Enter number: ";
    cin >> number;
    if (number % factors(number) == 0) {
        cout << factors(number) << " is the highest factor!" << endl;
