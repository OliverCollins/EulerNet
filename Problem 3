//Largest prime factor
//Unsolved

#include <iostream>

#include <iostream>
using namespace std;

int prime(int number) {
    int prime = 2;
    bool boo = true;
    while (boo) {
        if (number % prime == 0) {
            boo = false;
            return number;
        }
    }
}

int main () {
    int number;
    cout << "Number: ";
    cin >> number;
    while (number != prime(number)) {
        cout << prime(number) << endl;
    }
    return 0;
}
