//Largest prime factor
//Unsolved

#include <iostream>
#include <string>
#include <sstream>

using namespace std;


int prime(int number) {
    int asd;
    int prime = 2;
    int x = 2;
    bool boo = false;
    int prime_counter = 0;
    if (number == 2) {
        boo = true;
        return boo;
    }
    while (number != prime_counter) {
        while ((number % x != 0) && (number > x) && !boo) {
            if ((number - 1) == x) {
                cout << number << " is prime" << endl;
                boo = true;
                prime_counter++;
                asd = number;
            } else {
                x++;
            }
        }
    } return asd;
}

int main () {
    int number;
    cout << "l: ";
    cin >> number;
    cout << prime(number);
    return 0;
}
