/*

//Large Prime Numbers
//Unsolved

#include <stdio.h>

int prime(int x) {
	bool flag = true;
	for (int i = 2; i < x; i++) {
		if (x / i == 0) {
			flag = false;
			break;
		}
	}
	return flag;	
}

int main(void) {
	unsigned int x = 1;
	for (int i = 1; i <= 10001; i++) {
		if (flag) {
			print("%d\n", x);
		}
	}
	return 0;
}
