/*

//Large Prime Numbers
//Unsolved

#include <stdio.h>
typedef int bool;
#define true 1
#define false 0

int prime(int x) {
	bool flag = true;
	for (int i = 2; i < x; i++) {
		if (x % i == 0) {
			flag = false;
			break;
		} else {
			
		}
	}
	return flag;	
}

int main(void) {
	unsigned int x = 3;
	for (int i = 1; i <= 10002; i++) {
		if (prime(x)) {
			if (i == 10002) {
				printf("%d", x);
			} else {
				x++;
			}
		} else {
			x++;
		}
	}
	return 0;
}
