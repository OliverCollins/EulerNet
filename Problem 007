importPackage(java.io);
importPackage(java.lang);

var prime = 3;
for (var i = 2; i < prime; i++) {
	if (prime % i == 0) { 	//Not a prime
		prime += 1; 		//Try next number up
		break; 				//Break out of for loop
	} 
}
