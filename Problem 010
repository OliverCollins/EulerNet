/*

//Summation of Primes
//Unsolved

#include <iostream>

using namespace std;

int prime(int const n) {
    long long p_count = 2, p = 3, x;
    while (n > p) {
        for (x = 2; x <= p - 1; ++x) {
            if (p % x == 0) {
                break;
            }
        } if (p == x) {
            p_count += p;
        } ++p;
    } return p_count;
}

int main() {
    int n;
    cout << "Select a number: ";
    cin >> n;
    cout << prime(n);
}
