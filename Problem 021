#include <stdio.h>

int sum_array(int a[], int num_elements) {
   int i, sum = 0;
   for (i = 0; i <num_elements; i++) {
	 sum = sum + a[i];
   }
   return(sum);
}

int amicable(int x) {
	int div = 1, int count = 0;
	int a = [x];
	for (int i = 1; i < x; i++) {
		if (x % i == 0) {
			a[count] = i;
			count++;
		} 
	} sum_array(a[], x);
}

