#include <iostream>
#include <string>
#include <sstream>

using namespace std;


int main () {
    int a = 1;
    int b = 2;
    int c = 3;
    while (a + b + c != 1000) {
        if (sqrt((a * a) + (b * b)) == c) {
            //Pythagorean Triple!
        }
    } cout << (a * b * c);
    
}


